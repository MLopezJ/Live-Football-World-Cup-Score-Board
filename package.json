{
  "name": "live-football-world-cup-scoreboard",
  "version": "0.0.0-development",
  "description": "A Live Football World Cup Scoreboard library that shows all the ongoing matches and their scores.",
  "main": "index.js",
  "scripts": {
    "test": "find ./ -type f -name '*.spec.ts' -not -path './node_modules/*' -print | xargs npx tsx --no-warnings --test",
    "prepare": "husky",
    "prepublishOnly": "npx swc -d dist ./src --copy-files"
  },
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MLopezJ/Live-Football-World-Cup-Scoreboard.git"
  },
  "keywords": [
    "node.js",
    "TDD",
    "OOP",
    "TypeScript"
  ],
  "author": "Mauro Lopez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MLopezJ/Live-Football-World-Cup-Scoreboard/issues"
  },
  "homepage": "https://github.com/MLopezJ/Live-Football-World-Cup-Scoreboard#readme",
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "18.4.3",
    "@types/node": "20.10.2",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "husky": "9.0.11",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "tsx": "4.6.1",
    "typescript": "5.3.2",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.2"
  }
